{
  "name": "coffeewip-slack",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "author": "CoffeeWIP",
  "license": "MIT",
  "dependencies": {
    "jest": "^26.1.0",
    "jscpd": "^3.3.0-rc.10",
    "typescript": "^3.9.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "jscpd": {
    "mode": "mild",
    "reporters": [
      "console",
      "time"
    ],
    "ignore": [
      "**/__tests__/**",
      "doc",
      "dist",
      "example"
    ],
    "absolute": true,
    "gitignore": true
  }
}
